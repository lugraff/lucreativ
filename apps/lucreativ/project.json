{
  "name": "lucreativ",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/lucreativ/src",
  "prefix": "lucreativ",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "routputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/lucreativ",
        "index": "apps/lucreativ/src/index.html",
        "main": "apps/lucreativ/src/main.ts",
        "polyfills": "apps/lucreativ/src/polyfills.ts",
        "tsConfig": "apps/lucreativ/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/lucreativ/src/favicon.ico",
          "apps/lucreativ/src/assets"
        ],
        "styles": [
          "apps/lucreativ/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "1mb",
              "maximumError": "2mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/lucreativ/src/environments/environment.ts",
              "with": "apps/lucreativ/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        },
        "for-backend": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "outputPath": "D:/ELITEIB_Beta/foWEb/lucreativ/dist/"
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "lucreativ:build:production"
        },
        "development": {
          "browserTarget": "lucreativ:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "lucreativ:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/lucreativ/**/*.ts",
          "apps/lucreativ/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/lucreativ"
      ],
      "options": {
        "jestConfig": "apps/lucreativ/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": [
    "type:app",
    "domain:main"
  ]
}