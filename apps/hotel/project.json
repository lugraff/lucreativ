{
  "name": "hotel",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/hotel/src",
  "prefix": "hotel",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "routputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/hotel",
        "index": "apps/hotel/src/index.html",
        "main": "apps/hotel/src/main.ts",
        "polyfills": "apps/hotel/src/polyfills.ts",
        "tsConfig": "apps/hotel/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/hotel/src/favicon.ico", "apps/hotel/src/assets"],
        "styles": ["apps/hotel/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "1mb",
              "maximumError": "2mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/hotel/src/environments/environment.ts",
              "with": "apps/hotel/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        },
        "for-backend": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "outputPath": "D:/ELITEIB_Beta/foWEb/hotel/dist/"
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "hotel:build:production"
        },
        "development": {
          "browserTarget": "hotel:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "hotel:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/hotel/**/*.ts", "apps/hotel/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/hotel"],
      "options": {
        "jestConfig": "apps/hotel/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["type:app", "domain:main"]
}
